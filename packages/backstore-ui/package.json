{
  "name": "@lamk/backstore-ui",
  "version": "1.0.34",
  "private": true,
  "scripts": {
    "dev": "concurrently \"react-app-rewired start\" \"nodemon --config nodemon.deps.json\"",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "depcruise": "dependency-cruise -T dot -x node_modules -v -- src/index.tsx  | dot -T png > code-architecture.png",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "aida": "aida run",
    "i18n": "node ./node_modules/@sradevski/translations/dist/index.js '[\"src/**/*.{ts,tsx}\"]' 'public/locales'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run i18n"
    }
  },
  "lint-staged": {
    "./**/*.{js,mjs,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.5",
    "@sradevski/blocks-ui": "0.0.47",
    "@sradevski/la-sdk": "0.0.67",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.28",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^5.0.0",
    "@types/styled-system": "^5.1.7",
    "antd": "^4.1.0",
    "compressorjs": "^1.0.6",
    "connected-react-router": "^6.7.0",
    "customize-cra": "^0.9.1",
    "date-fns": "^2.9.0",
    "env-var": "^6.0.1",
    "i18next": "^19.3.1",
    "i18next-browser-languagedetector": "^4.0.2",
    "i18next-xhr-backend": "^3.2.2",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.11",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.3.3",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.0.1",
    "styled-system": "^5.1.5",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@aida/cli": "0.0.7",
    "@aida/injector-faked-routes": "0.0.6",
    "@aida/injector-open-api": "0.0.6",
    "@aida/injector-routes": "0.0.6",
    "@aida/injector-routes-map": "0.0.6",
    "@sradevski/translations": "0.0.35",
    "@types/socket.io-client": "^1.4.32",
    "babel-plugin-import": "^1.13.0",
    "concurrently": "^5.1.0",
    "dependency-cruiser": "^7.2.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "source-map-explorer": "^2.3.1"
  }
}
