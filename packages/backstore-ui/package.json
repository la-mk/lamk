{
  "name": "@lamk/backstore-ui",
  "version": "1.0.82",
  "private": true,
  "scripts": {
    "dev": "concurrently \"react-app-rewired start\" \"nodemon --config nodemon.deps.json\"",
    "build": "GENERATE_SOURCEMAP=false && react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "depcruise": "dependency-cruise -T dot -x node_modules -v -- src/index.tsx  | dot -T png > code-architecture.png",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "aida": "aida run",
    "i18n": "node ./node_modules/@sradevski/translations/dist/index.js '[\"src/**/*.{ts,tsx}\"]' 'public/locales'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run i18n"
    }
  },
  "lint-staged": {
    "./**/*.{js,mjs,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@jaames/iro": "^5.2.3",
    "@react-pdf/renderer": "^2.0.0-beta.6",
    "@sradevski/analytics": "0.0.9",
    "@sradevski/blocks-ui": "0.0.120",
    "@sradevski/la-sdk": "0.0.129",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.11.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.3",
    "@types/styled-system": "^5.1.10",
    "antd": "^4.6.5",
    "compressorjs": "^1.0.6",
    "connected-react-router": "^6.8.0",
    "customize-cra": "^1.0.0",
    "date-fns": "^2.16.1",
    "env-var": "^6.3.0",
    "i18next": "^19.7.0",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "immutable": "^4.0.0-rc.12",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "lodash": "^4.17.20",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^16.13.1",
    "react-error-boundary": "^3.0.2",
    "react-i18next": "^11.7.2",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "recharts": "^1.8.5",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "seamless-immutable": "^7.1.4",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.2.0",
    "styled-system": "^5.1.5",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@sradevski/translations": "0.0.101",
    "@types/recharts": "^1.8.15",
    "@types/socket.io-client": "^1.4.33",
    "babel-plugin-import": "^1.13.0",
    "concurrently": "^5.3.0",
    "dependency-cruiser": "^9.14.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "source-map-explorer": "^2.5.0"
  }
}
