{
  "name": "@lamk/backstore-ui",
  "version": "1.0.109",
  "private": true,
  "scripts": {
    "dev": "react-app-rewired start",
    "build": "export GENERATE_SOURCEMAP=false && react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "depcruise": "dependency-cruise -T dot -x node_modules -v -- src/index.tsx  | dot -T png > code-architecture.png",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "aida": "aida run",
    "i18n": "node ./node_modules/@la-mk/translations/dist/index.js '[\"src/**/*.{ts,tsx}\"]' 'public/locales'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run i18n"
    }
  },
  "lint-staged": {
    "./**/*.{js,mjs,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@la-mk/analytics": "0.0.13",
    "@la-mk/blocks-ui": "0.0.285",
    "@la-mk/la-sdk": "0.0.166",
    "@la-mk/translations": "0.0.132",
    "@react-pdf/renderer": "^2.0.0",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@types/styled-system": "^5.1.11",
    "antd": "^4.10.0",
    "compressorjs": "^1.0.7",
    "connected-react-router": "^6.9.1",
    "customize-cra": "^1.0.0",
    "date-fns": "^2.19.0",
    "env-var": "^7.0.0",
    "i18next": "^19.8.4",
    "i18next-browser-languagedetector": "^6.1.0",
    "i18next-xhr-backend": "^3.2.2",
    "immutable": "^4.0.0-rc.12",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "lodash": "^4.17.20",
    "react": "^17.0.2",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-i18next": "^11.8.12",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "recharts": "^1.8.5",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "seamless-immutable": "^7.1.4",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.2.3",
    "styled-system": "^5.1.5",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/recharts": "^1.8.19",
    "@types/socket.io-client": "^1.4.36",
    "babel-plugin-import": "^1.13.3",
    "dependency-cruiser": "^9.25.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "source-map-explorer": "^2.5.2"
  }
}
