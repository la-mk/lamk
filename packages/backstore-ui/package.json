{
  "name": "@lamk/backstore-ui",
  "version": "1.0.20",
  "private": true,
  "scripts": {
    "dev": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "depcruise": "dependency-cruise -T dot -x node_modules -v -- src/index.tsx  | dot -T png > code-architecture.png",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "aida": "aida run",
    "i18n": "node ./node_modules/@sradevski/translations/dist/index.js '[\"src/**/*.{ts,tsx}\"]' 'public/locales'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run i18n"
    }
  },
  "lint-staged": {
    "./**/*.{js,mjs,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@sradevski/blocks-ui": "0.0.31",
    "@sradevski/la-sdk": "0.0.23",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.146",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^4.3.4",
    "@types/styled-components": "^4.4.0",
    "@types/styled-system": "^5.1.3",
    "antd": "^3.26.7",
    "compressorjs": "^1.0.6",
    "connected-react-router": "^6.5.2",
    "customize-cra": "^0.5.0",
    "date-fns": "^2.9.0",
    "env-var": "^5.1.0",
    "i18next": "^19.0.0",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.11",
    "react": "^16.11.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.11.0",
    "react-i18next": "^11.2.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.2.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "socket.io-client": "^2.3.0",
    "styled-components": "^4.4.1",
    "styled-system": "^5.1.2",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@aida/cli": "0.0.7",
    "@aida/injector-faked-routes": "0.0.6",
    "@aida/injector-open-api": "0.0.6",
    "@aida/injector-routes": "0.0.6",
    "@aida/injector-routes-map": "0.0.6",
    "@sradevski/translations": "0.0.16",
    "@types/socket.io-client": "^1.4.32",
    "babel-plugin-import": "^1.12.2",
    "dependency-cruiser": "^5.2.3",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.19.1",
    "source-map-explorer": "^2.1.0"
  }
}
