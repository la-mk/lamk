{
  "name": "backstore-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "PORT=4040 react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "depcruise": "dependency-cruise -T dot -x node_modules -v -- src/index.tsx  | dot -T png > code-architecture.png",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "aida": "aida run"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,mjs,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.134",
    "@types/node": "^12.7.3",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.0",
    "@types/react-router-dom": "^4.3.4",
    "@types/styled-components": "^4.1.16",
    "@types/styled-system": "^5.1.1",
    "antd": "^3.21.4",
    "blocks-ui": "file:blocks-ui-1.10.1.tgz",
    "connected-react-router": "^6.5.2",
    "customize-cra": "^0.5.0",
    "env-var": "^5.1.0",
    "la-sdk": "file:la-sdk-1.6.11.tgz",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.1.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.0.3",
    "reselect": "^4.0.0",
    "socket.io-client": "^2.2.0",
    "styled-components": "^4.2.0",
    "styled-system": "^5.0.12",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@aida/cli": "0.0.7",
    "@aida/injector-faked-routes": "0.0.6",
    "@aida/injector-open-api": "0.0.6",
    "@aida/injector-routes": "0.0.6",
    "@aida/injector-routes-map": "0.0.6",
    "@types/socket.io-client": "^1.4.32",
    "babel-plugin-import": "^1.12.0",
    "dependency-cruiser": "^5.0.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "prettier": "^1.17.0",
    "source-map-explorer": "^2.0.1"
  }
}
