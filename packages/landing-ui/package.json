{
  "name": "@lamk/landing-ui",
  "version": "1.0.1",
  "description": "Landing UI",
  "main": "pages/index.ts",
  "author": "Stevche Radevski <sradevski@live.com>",
  "license": "ISC",
  "scripts": {
    "dev": "next -p 7001",
    "build": "next build && next export",
    "start": "next start",
    "optimize-images": "svgo -f ./public",
    "i18n": "node ./node_modules/@sradevski/translations/dist/index.js '[\"src/**/*.{ts,tsx}\", \"pages/**/*.{ts,tsx}\"]' 'public/locales'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run i18n"
    }
  },
  "lint-staged": {
    "./**/*.{js,mjs,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@sradevski/analytics": "0.0.9",
    "@sradevski/blocks-ui": "0.0.242",
    "env-var": "^7.0.0",
    "next": "^10.0.4",
    "next-i18next": "^7.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@sradevski/translations": "0.0.111",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@types/styled-system": "^5.1.10",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "next-compose-plugins": "^2.2.1",
    "next-transpile-modules": "^6.0.0",
    "prettier": "^2.2.1",
    "svgo": "^1.3.2",
    "typescript": "^4.1.3"
  }
}
