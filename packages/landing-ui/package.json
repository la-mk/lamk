{
  "name": "@lamk/landing-ui",
  "version": "1.0.1",
  "description": "Landing UI",
  "main": "pages/index.ts",
  "author": "Stevche Radevski <sradevski@live.com>",
  "license": "ISC",
  "scripts": {
    "dev": "next -p 7001",
    "build": "next build && next export",
    "start": "next start",
    "optimize-images": "npm run optimize-svg && npm run optimize-png",
    "optimize-svg": "svgo -f ./public",
    "optimize-png": "find ./public -type f -name '*.png' | while read fname; do imagemin $fname > ${fname}.tmp && mv ${fname}.tmp ${fname}; done;",
    "i18n": "node ./node_modules/@la-mk/translations/dist/index.js '[\"src/**/*.{ts,tsx}\", \"pages/**/*.{ts,tsx}\"]' 'public/locales'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run i18n"
    }
  },
  "lint-staged": {
    "./**/*.{js,mjs,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@la-mk/analytics": "0.0.10",
    "@la-mk/blocks-ui": "0.0.273",
    "@la-mk/translations": "0.0.122",
    "date-fns": "^2.17.0",
    "env-var": "^7.0.0",
    "front-matter": "^4.0.2",
    "next": "^10.0.6",
    "next-i18next": "^7.0.1",
    "next-seo": "^4.19.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.19",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@types/styled-system": "^5.1.10",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.6",
    "imagemin-cli": "^6.0.0",
    "lint-staged": "^10.5.3",
    "next-compose-plugins": "^2.2.1",
    "next-transpile-modules": "^6.3.0",
    "prettier": "^2.2.1",
    "svgo": "^1.3.2",
    "typescript": "^4.1.3"
  }
}
