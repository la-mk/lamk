{
  "name": "@lamk/store-ui",
  "version": "1.0.99",
  "description": "Store UI",
  "main": "pages/index.ts",
  "author": "Stevche Radevski <sradevski@live.com>",
  "license": "ISC",
  "scripts": {
    "dev": "concurrently \"nodemon --config nodemon.src.json\" \"nodemon --config nodemon.deps.json \"",
    "build": "next build && tsc --project tsconfig.server.json",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "i18n": "node ./node_modules/@sradevski/translations/dist/index.js '[\"src/**/*.{ts,tsx}\", \"pages/**/*.{ts,tsx}\"]' 'public/locales'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run i18n"
    }
  },
  "lint-staged": {
    "./**/*.{js,mjs,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@sradevski/analytics": "0.0.7",
    "@sradevski/blocks-ui": "0.0.113",
    "@sradevski/la-sdk": "0.0.119",
    "@sradevski/nlp": "0.0.2",
    "antd": "^4.5.2",
    "bezier-easing": "^2.1.0",
    "connected-next-router": "^3.1.0",
    "date-fns": "^2.15.0",
    "env-var": "^6.3.0",
    "express": "^4.17.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.19",
    "mem": "^6.1.0",
    "next": "^9.3.6",
    "next-i18next": "^4.4.2",
    "next-redux-wrapper": "^5.0.0",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.5",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "@sradevski/translations": "0.0.94",
    "@types/express": "^4.17.7",
    "@types/next-redux-wrapper": "^3.0.0",
    "@types/node": "^14.0.27",
    "@types/qs": "^6.9.4",
    "@types/react": "^16.9.44",
    "@types/styled-components": "^5.1.2",
    "@types/styled-system": "^5.1.10",
    "@typescript-eslint/parser": "^3.8.0",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-less": "^1.0.1",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-styled-components": "^1.11.1",
    "concurrently": "^5.2.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "less": "^3.12.2",
    "lint-staged": "^10.2.11",
    "next-compose-plugins": "^2.2.0",
    "next-transpile-modules": "^4.0.2",
    "nodemon": "^2.0.4",
    "null-loader": "^4.0.0",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
