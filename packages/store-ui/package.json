{
  "name": "@lamk/store-ui",
  "version": "1.0.169",
  "description": "Store UI",
  "main": "pages/index.ts",
  "author": "Stevche Radevski <sradevski@live.com>",
  "license": "ISC",
  "scripts": {
    "dev": "next dev --port $PORT",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "i18n": "node ./node_modules/@la-mk/translations/dist/index.js '[\"src/**/*.{ts,tsx}\", \"pages/**/*.{ts,tsx}\"]' 'public/locales'",
    "postinstall": "patch-package"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run i18n"
    }
  },
  "lint-staged": {
    "./**/*.{js,mjs,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@la-mk/analytics": "0.0.13",
    "@la-mk/blocks-ui": "0.0.310",
    "@la-mk/la-sdk": "0.0.174",
    "@la-mk/nlp": "0.0.4",
    "@la-mk/translations": "0.0.144",
    "connected-next-router": "^4.0.1",
    "date-fns": "^2.19.0",
    "env-var": "^7.0.0",
    "i18n": "^0.13.2",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "mem": "^8.1.0",
    "next": "^10.1.3",
    "next-i18next": "^7.0.1",
    "next-redux-wrapper": "^6.0.2",
    "next-seo": "^4.23.0",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "tinycolor2": "^1.4.2"
  },
  "devDependencies": {
    "@types/next-redux-wrapper": "^3.0.0",
    "@types/node": "^14.14.37",
    "@types/qs": "^6.9.6",
    "@types/react": "^17.0.3",
    "@typescript-eslint/parser": "^4.21.0",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "eslint": "^7.23.0",
    "lint-staged": "^10.5.4",
    "next-compose-plugins": "^2.2.1",
    "next-transpile-modules": "^6.4.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
