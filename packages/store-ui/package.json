    "@sradevski/nlp": "0.0.3",
    "connected-next-router": "^4.0.1",
    "date-fns": "^2.16.1",
    "env-var": "^7.0.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.20",
    "mem": "^8.0.0",
    "next": "^10.0.3",
    "next-i18next": "^7.0.1",
    "next-redux-wrapper": "^6.0.2",
    "qs": "^6.9.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
