    "@types/styled-components": "^5.1.5",
    "@types/styled-system": "^5.1.10",
    "@typescript-eslint/parser": "^4.9.1",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "eslint": "^7.15.0",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "next-compose-plugins": "^2.2.1",
    "next-transpile-modules": "^6.0.0",
